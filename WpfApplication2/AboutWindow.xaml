            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">

                        <Viewbox Width="{TemplateBinding Width}" 
                                 Height="{TemplateBinding Height}" ClipToBounds="True" >
